class Solution {
    public int fibo(int n) {
        int answer=0;
        if(n>=2) {
            answer = fibo(n-1) + fibo(n-2);
        }
        else if(n==1) {
            answer = 1;
        }
        else if(n==0) {
            answer = 0;
        }
        return answer;
    }

    public int solution(int n) {
        int answer = 0;
        answer = fibo(n)%1234567;
        return answer;
    }
    
}
